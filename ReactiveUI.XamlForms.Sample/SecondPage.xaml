<?xml version="1.0" encoding="utf-8" ?>
<d:ReactiveContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="ReactiveUI.XamlForms.Sample.SecondPage"
             xmlns:d="clr-namespace:ReactiveUI.XamForms;assembly=ReactiveUI.XamForms"
             xmlns:vm="clr-namespace:ReactiveUI.XamlForms.Sample.ViewModels;assembly=ReactiveUI.XamlForms.Sample"
             xmlns:local="clr-namespace:ReactiveUI.XamlForms.Sample"
             x:TypeArguments="vm:SecondPageViewModel">
    <Grid VerticalOptions="Fill">
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto" />
            <RowDefinition Height="Auto"  />
            <RowDefinition Height="*"  />
        </Grid.RowDefinitions>
        <Label Grid.Row="0" x:Name="lbl" VerticalOptions="Center" HorizontalOptions="Center" />
        <Button Grid.Row="1" x:Name="btn" Text="Go Back"></Button>
        <local:GridView Grid.Row="2" x:Name="gridView"></local:GridView>
    </Grid>
</d:ReactiveContentPage>